  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-css-modules-transform": "^0.1.1",
    "babel-plugin-react-remove-prop-types": "^2.0.2",
    "babel-plugin-react-require": "^2.1.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-dead-code-elimination": "^2.0.2",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-router-optimize": "^1.0.1",
    "babel-preset-decorators-legacy": "^1.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-meteor": "^6.11.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-traverse": "^6.11.4",
    "gagarin": "^0.4.12",
    "norma-bower": "^0.3.8",
    "norma-meteor-run": "jbaxleyiii/norma-meteor-run"
  },
  "babel": {
    "compact": true,
    "presets": [
      "meteor",
      "es2015-loose",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-react-router-optimize",
      [
        "react-transform",
        {
          "transforms": [
            {
              "transform": "react-transform-sentry",
              "imports": [
                "react"
              ]
            }
          ]
        }
      ],
      "transform-inline-environment-variables",
      "transform-react-constant-elements",
      "transform-decorators-legacy",
      "react-require",
      "react-remove-prop-types",
      "transform-dead-code-elimination"
    ]
  },
  "apollos": {
    "settings": "./.remote/settings/sites/app.newspring.io/alpha.settings.json",
    "resource": {
      "./.remote": {
        "junction": "NewSpring/junction-framework",
        "fonts": "NewSpring/newspring-fonts"
      },
      "./packages": {
        "meteor-scss": "https://github.com/NewSpring/meteor-scss.git#432a60bded46edd02561e87b6a947b88072e9c68",
        "fetch": "https://github.com/NewSpring/meteor-fetch.git#c36567418edd59564d54de9747c1f59e0ed0bf7f",
        "inject-data": "https://github.com/NewSpring/inject-data.git#ea2e078e4763f8fd3875c77fb1073e3794bf8e6a"
      }
    }
  },
  "cssModules": {
    "specificArchitecture": false
  },
  "dependencies": {
    "aphrodite": "git+https://github.com/NewSpring/aphrodite.git#release",
    "apollo-client": "^0.3.27",
    "apollos-core": "../../apollos",
    "audio5": "^0.1.10",
    "graphql-tag": "^0.1.8",
    "hammerjs": "2.0.4",
    "meteor-node-stubs": "^0.2.3",
    "moment": "^2.14.1",
    "react": "^15.0.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-apollo": "^0.3.17",
    "react-async-script-loader": "^0.2.1",
    "react-dom": "^15.0.0",
    "react-hammerjs": "^0.4.6",
    "react-helmet": "^3.1.0",
    "react-mixin": "^3.0.5",
    "react-motion": "^0.4.4",
    "react-pull-to-refresh": "git+https://git@github.com/NewSpring/react-pull-to-refresh.git",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.0",
    "react-slick": "^0.13.1",
    "react-swipe-views": "git+https://git@github.com/NewSpring/react-swipe-views.git#d531949ddccb84ec53e634f7964c1c5cfac2c9b5",
    "react-tooltip": "^3.0.13",
    "react-transform-sentry": "^1.0.0",
    "redux": "^3.0.4",
    "truncate": "^2.0.0"
  }
}
