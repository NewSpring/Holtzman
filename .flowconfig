[ignore]
.*/node_modules/.*
.*/\.meteor/.*
.*/packages/.*
.*/__tests__/.*
.*/\.bin/.*
.*/\.build/.*
.*/\.git/.*
.*/\.github/.*
.*/\.remote/.*
.*/\.storybook/.*
.*/\.travis/.*
.*/\.vscode/.*
.*/public/.*
.*/scripts/.*
.*/stylesheets/.*

[include]

[libs]
.types/

[options]
module.name_mapper='^\/\(.*\)$' -> '<PROJECT_ROOT>/\1'

# Meteor core package support
# e.g. "meteor/meteor"
module.name_mapper='^meteor\/\(.*\)$' -> '<PROJECT_ROOT>/.meteor/local/build/programs/server/packages/\1'


# Meteor ignore
suppress_comment= \\(.\\|\n\\)*\\$FlowMeteor
